mas production_department {
    agent carl: production_supervisor.asl {
        beliefs: client(client_carl)
                 message_box(message_box_carl)

        goal: supervise

        focus: managers.message_box_carl
    }

    workspace managers {
        artifact client_carl: org.hyperagents.demo.ProductionSupervisorClient("carl", "http://localhost:8081", "http://localhost:8080")
        artifact message_box_carl: org.hyperagents.demo.MessageBox(8081)

        agents: carl
    }

    asl-path: "src/main/jason"
    java-path: "src/main/cartago"
    org-path: "src/main/moise"
}